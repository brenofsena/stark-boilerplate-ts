{
  "name": "stark-boilerplate-ts",
  "version": "1.0.0",
  "description": "A simple boilerplate for React applications with TypeScript",
  "scripts": {
    "start": "node index",
    "dev:base": "webpack-dev-server --config webpack.dev.js",
    "dev": "npm run dev:base -- --progress --open",
    "build": "webpack --config webpack.prod.js",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info",
    "test:cypress": "cypress open",
    "test:cypress:run": "cypress run",
    "test:cypress:ci": "start-server-and-test dev:base http-get://localhost:3333 test:cypress:run",
    "check": "npm-check -u -s",
    "generate": "plop --plopfile generators/plopfile.js"
  },
  "author": "Breno Sena",
  "license": "MIT",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.4.5",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "@testing-library/react-hooks": "^3.4.1",
    "@types/faker": "^5.1.0",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.1.0",
    "css-loader": "^4.2.2",
    "cypress": "^5.1.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-standard": "^4.0.1",
    "faker": "^5.1.0",
    "favicons-webpack-plugin": "^4.2.0",
    "file-loader": "^6.0.0",
    "git-commit-msg-linter": "^2.5.2",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "jest": "^26.4.2",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^10.2.11",
    "npm-check": "^5.9.2",
    "offline-plugin": "^5.0.7",
    "plop": "^2.7.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.0.0",
    "react-test-renderer": "^16.13.1",
    "start-server-and-test": "^1.11.3",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^5.1.2",
    "webpack-pwa-manifest": "^4.1.1"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^2.0.0",
    "express": "^4.17.1",
    "express-history-api-fallback": "^2.2.1",
    "polished": "^3.6.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.1.1"
  }
}
